{
    "author": "josh Goldberg",
    "bin": {
        "typestat": "bin/typestat"
    },
    "dependencies": {
        "@phenomnomnominal/tsquery": "^3.0.0",
        "@types/mz": "0.0.32",
        "automutate": "^0.7.2",
        "builtin-modules": "^3.0.0",
        "chalk": "^2.4.2",
        "commander": "^2.19.0",
        "cosmiconfig": "^5.1.0",
        "glob": "^7.1.3",
        "mz": "^2.7.0",
        "tsutils": "^3.8.0",
        "typescript": "^3.3.3333"
    },
    "description": "Adds and improves TypeScript type annotations using static analysis.",
    "devDependencies": {
        "@types/builtin-modules": "2.0.0",
        "@types/cosmiconfig": "5.0.3",
        "@types/glob": "7.1.1",
        "@types/jest": "24.0.11",
        "@types/node": "11.13.4",
        "@types/prop-types": "15.7.1",
        "@types/react": "16.8.13",
        "automutate-tests": "0.3.5",
        "chai": "4.2.0",
        "husky": "1.3.1",
        "jest": "24.7.1",
        "markdownlint": "0.13.0",
        "markdownlint-cli": "0.15.0",
        "mocha": "6.1.3",
        "npm-run-all": "4.1.5",
        "prettier": "1.17.0",
        "pretty-quick": "1.10.0",
        "sentences-per-line": "0.2.1",
        "ts-jest": "24.0.2",
        "tslint": "5.16.0",
        "tslint-config-prettier": "1.18.0"
    },
    "engines": {
        "node": "^8 || ^9 || ^10 || ^11 || ^12"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --write"
        }
    },
    "license": "MIT",
    "main": "src/index.js",
    "name": "typestat",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JoshuaKGoldberg/TypeStat.git"
    },
    "scripts": {
        "lint": "run-p lint:*",
        "lint:markdownlint": "markdownlint --config .markdownlint.json --rules ./node_modules/sentences-per-line/index.js README.md",
        "lint:tslint": "tslint -p tsconfig.json -t stylish",
        "prettier:write": "prettier --config .prettierrc.json --write",
        "prettier:write:all": "yarn run prettier:write ./{src,test,util}/**/*.{json,ts}",
        "test": "yarn run test:unit && yarn run test:mutation",
        "test:mutation": "mocha src/tests/runTests.js --timeout 10000",
        "test:unit": "jest",
        "tsc": "tsc",
        "verify": "yarn run tsc && yarn run lint && yarn run test"
    },
    "version": "0.2.1"
}
