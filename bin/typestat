#!/usr/bin/env node
// tslint:disable

const { runCli } = require("../src/cli/runCli");

runCli(process.argv)
    .then((resultStatus) => {
        if (resultStatus !== 0) {
            process.exitCode = 1;
        }
    })
    .catch((error) => {
        process.stderr.write("Error in TypeStat: " + error);
        process.stderr.write("\n");
        process.exitCode = 1;
    });
